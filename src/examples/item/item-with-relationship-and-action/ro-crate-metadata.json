            "participant": { "@id": "#participant1" },
            "agent": { "@id": "#agent1" }
        },
        {
            "@id": "#person2",
            "@type": ["Person"],
            "objectOf": { "@id": "#createAction1" }
        },
        {
            "@id": "#participant1",
            "@type": ["Thing"],
            "participantOf": { "@id": "#createAction1" }
        },
        {
            "@id": "#agent1",
